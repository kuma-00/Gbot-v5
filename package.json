{
  "name": "gbot-v5ranger",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "ts-start": "node --experimental-loader esbuild-node-loader src/main.ts",
    "start": "node build/main.js",
    "compile": "rm -rf ./build ;  tsc -p . && tsc-alias -p tsconfig.json",
    "compRes": "rm -rf ./build ;  tsc -p . && tsc-alias -p tsconfig.json && pm2 restart gbot-v5Ranger",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "update-data": "node --experimental-loader esbuild-node-loader src/createArtifactData.ts",
    "pm2-start": "pm2 start build/main.js --name gbot-v5Ranger",
    "pm2-restart": "pm2 restart gbot-v5Ranger",
    "pm2-stop": "pm2 stop gbot-v5Ranger",
    "pm2-logs": "pm2 logs gbot-v5Ranger --lines 100",
    "deploy":"npm run compile && flyctl deploy"
  },
  "engines": {
    "node": "18.x"
  },
  "keywords": [],
  "author": "kuma",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "esbuild-node-loader": "^0.8.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "@types/node": "^18.14.0",
    "@types/wav": "^1.0.1"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "app-root-path": "^3.1.0",
    "date-fns": "^2.29.3",
    "deta": "^1.1.0",
    "discord-api-types": "^0.37.35",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "fuzzball": "^2.1.2",
    "genshin-db": "^4.3.5",
    "music-metadata": "^8.1.3",
    "node-fetch": "^3.3.0",
    "ocr-space-api-wrapper": "^2.2.0",
    "pm2": "^5.2.2",
    "sodium-native": "^4.0.1",
    "tweetnacl": "^1.0.3",
    "vm2": "^3.9.14",
    "wakachigaki": "^1.3.2",
    "wav": "^1.0.2",
    "wikipedia": "^2.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "useESM": true
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@src/(.+)": "<rootDir>/src/$1"
    },
    "preset": "ts-jest/presets/default-esm"
  }
}

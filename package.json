    "update-data": "node --experimental-loader esbuild-node-loader src/createArtifactData.ts"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [],
  "author": "kuma",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "esbuild-node-loader": "^0.8.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "app-root-path": "^3.0.0",
    "deta": "^1.1.0",
    "discord-api-types": "^0.34.0",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "ffmpeg-static": "^5.0.2",
    "fuzzball": "^2.1.2",
    "node-fetch": "^3.2.6",
    "ocr-space-api-wrapper": "^2.1.1",
    "tweetnacl": "^1.0.3",
    "vm2": "^3.9.9",
    "wikipedia": "^1.1.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json",
        "useESM": true
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@src/(.+)": "<rootDir>/src/$1"
    },
    "preset": "ts-jest/presets/default-esm"
  }
